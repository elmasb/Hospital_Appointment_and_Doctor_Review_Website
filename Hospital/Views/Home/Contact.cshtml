@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="contact">
    <div class="div">
        <div class="sub-head">
            <div class="overlap-contacts-locations-div">
                <div class="overlap-group">
                </div>
            </div>
            <div class="overlap-wrapper">
                <div class="image-text">
                    <div class="contact-us">Our Contacts</div>
                    <div class="home-contact">Home / Contact</div>
                </div>
            </div>
        </div>
    </div>
    <div class="google-map">
        <!-- <div class="mapview"></div> -->
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d96358.00904849908!2d28.770303823657205!3d40.999262923916504!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14caa35ee1fdc1b1%3A0x658a212e6b4359b5!2zVC5DLiDEsHN0YW5idWwgS8O8bHTDvHIgw5xuaXZlcnNpdGVzaQ!5e0!3m2!1str!2str!4v1700003340431!5m2!1str!2str" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    </div>
    <div class="form-contacts">

        <div class="form-contacts-div">
            <div class="contacts-locations-div">
                <div class="contacts-locations">
                    <img class="contacts-locations-image" src="https://c.animaapp.com/IQy7NEBf/img/group-178-1@2x.png" />
                    <div class="contacts-locations-text">
                        <div class="place-map">0123 Some place</div>
                        <a href="mailto:fildineesoe@gmail.com" target="_blank" rel="noopener noreferrer"><div class="country-map">9876 Some country</div></a>
                        <div class="location">LOCATIONS</div>
                    </div>
                </div>
            </div>
            <div class="contacts-emergency-div">
                <div class="contacts-emergency">
                    <img class="contacts-emergency-image" src="https://c.animaapp.com/IQy7NEBf/img/group-188-1@2x.png" />
                    <div class="contacts-emergency-text">
                        <div class="contact-info-1">(237) 681-812-255</div>
                        <a href="mailto:fildineesoe@gmail.com" target="_blank" rel="noopener noreferrer"><div class="contact-info-2">(237) 666-331-894</div></a>
                        <div class="emergency-email-working-hours">EMERGENCY</div>
                    </div>
                </div>
            </div>
            <div class="contacts-email-div">
                <div class="contacts-email">
                    <img class="contacts-email-image" src="https://c.animaapp.com/IQy7NEBf/img/group-202@2x.png" />
                    <div class="contacts-email-text">
                        <a href="mailto: fildineeesoe@gmil.com" class="contact-info-1">fildineeesoe@gmail.com</a>
                        <a href="mailto: myebstudios@gmail.com" class="contact-info-2-email">myebstudios@gmail.com</a>
                        <div class="emergency-email-working-hours">EMAIL</div>
                    </div>
                </div>
            </div>
            <div class="contacts-working-hours-div">
                <div class="contacts-working-hours">
                    <img class="contacts-working-hours-image" src="https://c.animaapp.com/IQy7NEBf/img/group-177-1@2x.png" />
                    <div class="contacts-working-hours-text">
                        <div class="contact-info-1">Mon-Sat 09:00-20:00</div>
                        <div class="contact-info-2">Sunday Emergency only</div>
                        <div class="emergency-email-working-hours">WORKING HOURS</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="wrapper">
            <header>Contact Us</header>
            <form id="myForm" action="#">
                <div class="dbl-field">
                    <div class="field">
                        <input type="text" name="name" placeholder="Enter your name">
                    </div>
                    <div class="field">
                        <input type="text" name="email" placeholder="Enter your email">

                    </div>
                </div>
                <div class="dbl-field">
                    <div class="field">
                        <input type="text" name="phone" placeholder="Enter your phone">

                    </div>
                    <div class="field">
                        <input type="text" name="website" placeholder="Enter your website">

                    </div>
                </div>
                <div class="message">
                    <textarea placeholder="Write your message" name="message"></textarea>
                </div>
                <div class="button-area">
                    <button type="submit">Send Message</button>

                </div>
            </form>
        </div>
        <script src="sub.js"></script>
